{
  "name": "EventsManager",
  "version": "0.0.0",
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    }
  },
  "private": true,
  "engines": {
    "node": "v7.7.3"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "nodemon ./server/dist/index.js --ignore ./server",
    "build": "babel server -d server/dist --copy-files --ignore server/dist,server/data,server/public,server/tests/",
    "postinstall": "npm run build",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "nyc --reporter=text --reporter=lcov mocha --timeout 10000 ./server/tests/test_users.js --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "express-validator": "^4.3.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.23.1",
    "sequelize-cli": "^3.1.0",
    "swagger-tools": "^0.10.3",
    "swagger-ui": "^3.5.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0"
  }
}
