{
  "name": "EventsManager",
  "version": "0.0.0",
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    }
  },
  "nyc": {
    "exclude": [
      "server/dist/**",
      "server/index.js"
    ]
  },
  "private": true,
  "engines": {
    "node": "v7.7.3"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "nodemon ./server/dist/index.js --ignore ./client",
    "build": "babel server -d server/dist --copy-files --ignore server/dist,server/data,server/tests/",
    "postinstall": "npm run build",
    "mocha": "mocha --compilers js:babel-core/register",
    "testServer": "nyc --reporter=text --reporter=lcov mocha --timeout 10000 --exit ./server/tests/*.test.js --require babel-core/register",
    "testClient": "mocha ./client/test/*.test.js --compilers js:babel-register --require ./client/test/setup.js",
    "test": "npm run testServer && npm run testClient",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "pack": "webpack -d && webpack-dev-server --hot"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "bootstrap": "^4.0.0-beta.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "express-validator": "^4.3.0",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.19.2",
    "morgan": "~1.9.0",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.4.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "sequelize": "^4.23.1",
    "sequelize-cli": "^3.1.0",
    "swagger-tools": "^0.10.3",
    "swagger-ui": "^3.5.0",
    "url-loader": "^0.6.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "copy-webpack-plugin-advanced": "^1.0.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "jsdom": "^11.6.2",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "redux-mock-provider": "^1.0.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
