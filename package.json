{
  "name": "EventsManager",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "v7.7.3"
  },
  "scripts": {
    "start": "nodemon ./server/dist/index.js --ignore ./server",
    "build": "babel server -d server/dist --ignore server/dist",
    "postinstall": "npm run build",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "nyc --reporter=text --reporter=lcov mocha --timeout 10000 ./server/tests/ --compilers js:babel-register",
    "coverage": "nyc report --reporter=lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "express-validator": "^4.3.0",
    "moment": "^2.19.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0"
  }
}
