{
  "name": "EventsManager",
  "version": "0.0.0",
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    }
  },
  "nyc": {
    "exclude": [
      "server/dist/**",
      "server/index.js"
    ]
  },
  "jest": {
    "testRegex": "client/test/.*\\.test\\.(js|jsx)$",
    "setupTestFrameworkScriptFile": "./client/test/setup.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/app/**",
      "!client/src/app/index.jsx"
    ]
  },
  "private": true,
  "scripts": {
    "start": "npm run webpack:prod && node ./server/dist/index.js --ignore ./client",
    "start:dev": "npm run webpack:dev & nodemon ./server/dist/index.js --ignore ./client",
    "build": "babel server -d server/dist --copy-files --ignore server/dist,server/tests/",
    "postinstall": "npm run build",
    "testServer": "NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --timeout 10000 --exit ./server/tests/index.js --require babel-core/register && npm run coverage && codeclimate-test-reporter < coverage/lcov.info",
    "testClient": "jest && codecov",
    "test": "npm run testServer && npm run testClient",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "webpack:dev": "webpack --config webpack.dev.js --watch",
    "webpack:prod": "webpack --config webpack.prod.js",
    "e2e": "nightwatch"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.3",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "bootstrap": "^4.0.0-beta.2",
    "codecov": "^3.0.2",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin-advanced": "^1.0.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expect": "^23.1.0",
    "express": "~4.15.5",
    "express-validator": "^4.3.0",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.10.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.19.2",
    "morgan": "~1.9.0",
    "nightwatch": "^0.9.21",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.4.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "rc-pagination": "^1.16.3",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.3.2",
    "redux": "^3.7.2",
    "sequelize": "^4.23.1",
    "sequelize-cli": "^3.1.0",
    "swagger-ui-express": "^3.0.8",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.6.2",
    "uuid": "^3.1.0",
    "webpack": "^4.4.1",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "jest": "^22.4.4",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
