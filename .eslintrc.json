{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "linebreak-style": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prefer-stateless-function": [0, { "ignorePureComponents": true }]
    },
    "env": {
    "node": true,
    "mocha": true,
    "es6": true,
    "browser": true
    },
    "settings": {
        "import/resolver": "node"
    }
}